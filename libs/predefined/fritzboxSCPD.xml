<?xml version="1.0" encoding="utf-8"?>
<scpd xmlns="urn:dslforum-org:service-1-0">
  <specVersion>
    <major>1</major>
    <minor>0</minor>
  </specVersion>
  <actionList>
    <action>
      <name>GetSwitchList</name>
      <description><![CDATA[Liefert die kommaseparierte AIN/MAC Liste aller bekannten Steckdosen
kommaseparierte AIN/MAC-Liste, leer wenn keine Steckdose bekannt]]></description>
      <argumentList>
         <argument>
          <name>sid</name>
          <direction>in</direction>
          <relatedStateVariable>sid</relatedStateVariable>
        </argument>
        <argument>
          <name>SwitchList</name>
          <direction>out</direction>
          <relatedStateVariable>SwitchList</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>setSwitchOn</name>
      <description><![CDATA[Schaltet Steckdose ein "1"]]></description>
      <argumentList>
        <argument>
          <name>AIN</name>
          <direction>in</direction>
          <relatedStateVariable>AIN</relatedStateVariable>
        </argument>
         <argument>
          <name>sid</name>
          <direction>in</direction>
          <relatedStateVariable>sid</relatedStateVariable>
        </argument>
        <argument>
          <name>state</name>
          <direction>out</direction>
          <relatedStateVariable>state</relatedStateVariable>
        </argument>
        <argument>
          <name>statetext</name>
          <direction>out</direction>
          <relatedStateVariable>statetext</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>setSwitchOff</name>
      <description><![CDATA[Schaltet Steckdose aus "0"]]></description>
      <argumentList>
        <argument>
          <name>AIN</name>
          <direction>in</direction>
          <relatedStateVariable>AIN</relatedStateVariable>
        </argument>
         <argument>
          <name>sid</name>
          <direction>in</direction>
          <relatedStateVariable>sid</relatedStateVariable>
        </argument>
        <argument>
          <name>state</name>
          <direction>out</direction>
          <relatedStateVariable>state</relatedStateVariable>
        </argument>
        <argument>
          <name>statetext</name>
          <direction>out</direction>
          <relatedStateVariable>statetext</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>setSwitchToggle</name>
      <description><![CDATA[Toggeln der Steckdose ein/aus
"0" oder "1" (Steckdose aus oder an)]]></description>
      <argumentList>
        <argument>
          <name>AIN</name>
          <direction>in</direction>
          <relatedStateVariable>AIN</relatedStateVariable>
        </argument>
         <argument>
          <name>sid</name>
          <direction>in</direction>
          <relatedStateVariable>sid</relatedStateVariable>
        </argument>
        <argument>
          <name>state</name>
          <direction>out</direction>
          <relatedStateVariable>state</relatedStateVariable>
        </argument>
        <argument>
          <name>statetext</name>
          <direction>out</direction>
          <relatedStateVariable>statetext</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>GetSwitchState</name>
      <description><![CDATA[Ermittelt Schaltzustand der Steckdose
"0" oder "1" (Steckdose aus oder an), "inval" wenn unbekannt]]></description>
      <argumentList>
        <argument>
          <name>AIN</name>
          <direction>in</direction>
          <relatedStateVariable>AIN</relatedStateVariable>
        </argument>
         <argument>
          <name>sid</name>
          <direction>in</direction>
          <relatedStateVariable>sid</relatedStateVariable>
        </argument>
        <argument>
          <name>state</name>
          <direction>out</direction>
          <relatedStateVariable>state</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>GetSwitchPresent</name>
      <description><![CDATA[Ermittelt Verbindungsstatus des Aktors
Bei Verbindungsverlust wechselt der Zustand erst mit einigen Minuten Verzögerung zu "0".
"0" oder "1" für Gerät nicht verbunden bzw. verbunden.]]></description>
      <argumentList>
        <argument>
          <name>AIN</name>
          <direction>in</direction>
          <relatedStateVariable>AIN</relatedStateVariable>
        </argument>
         <argument>
          <name>sid</name>
          <direction>in</direction>
          <relatedStateVariable>sid</relatedStateVariable>
        </argument>
        <argument>
          <name>state</name>
          <direction>out</direction>
          <relatedStateVariable>state</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>GetSwitchPower</name>
      <description><![CDATA[Ermittelt aktuell über die Steckdose entnommene Leistung
Leistung in mW, "inval" wenn unbekannt]]></description>
      <argumentList>
        <argument>
          <name>AIN</name>
          <direction>in</direction>
          <relatedStateVariable>AIN</relatedStateVariable>
        </argument>
         <argument>
          <name>sid</name>
          <direction>in</direction>
          <relatedStateVariable>sid</relatedStateVariable>
        </argument>
        <argument>
          <name>Power</name>
          <direction>out</direction>
          <relatedStateVariable>power</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>GetSwitchEnergy</name>
      <description><![CDATA[Liefert die über die Steckdose entnommene Ernergiemenge seit Erstinbetriebnahme oder Zurücksetzen der Energiestatistik
Energie in Wh, "inval" wenn unbekannt]]></description>
      <argumentList>
        <argument>
          <name>AIN</name>
          <direction>in</direction>
          <relatedStateVariable>AIN</relatedStateVariable>
        </argument>
         <argument>
          <name>sid</name>
          <direction>in</direction>
          <relatedStateVariable>sid</relatedStateVariable>
        </argument>
        <argument>
          <name>Energie</name>
          <direction>out</direction>
          <relatedStateVariable>power</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>GetSwitchName</name>
      <description><![CDATA[Liefert Bezeichner des Aktors Name]]></description>
      <argumentList>
        <argument>
          <name>AIN</name>
          <direction>in</direction>
          <relatedStateVariable>AIN</relatedStateVariable>
        </argument>
         <argument>
          <name>sid</name>
          <direction>in</direction>
          <relatedStateVariable>sid</relatedStateVariable>
        </argument>
        <argument>
          <name>Name</name>
          <direction>out</direction>
          <relatedStateVariable>name</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>GetDeviceListInfos</name>
      <description><![CDATA[Liefert die grundlegenden Informationen aller SmartHome-Geräte
XML-Format mit grundlegenden und funktionsspezifischen Informationen]]></description>
      <argumentList>
         <argument>
          <name>sid</name>
          <direction>in</direction>
          <relatedStateVariable>sid</relatedStateVariable>
        </argument>
        <argument>
          <name>Infos</name>
          <direction>out</direction>
          <relatedStateVariable>info</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>GetTemperature</name>
      <description><![CDATA[Letzte Temperaturinformation des Aktors
Temperatur-Wert in 0,1 °C, negative und positive Werte möglich
Bsp. „200“ bedeutet 20°C]]></description>
      <argumentList>
        <argument>
          <name>AIN</name>
          <direction>in</direction>
          <relatedStateVariable>AIN</relatedStateVariable>
        </argument>
         <argument>
          <name>sid</name>
          <direction>in</direction>
          <relatedStateVariable>sid</relatedStateVariable>
        </argument>
        <argument>
          <name>Temperatur</name>
          <direction>out</direction>
          <relatedStateVariable>temperatur</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>GetHkrtSoll</name>
      <description><![CDATA[Für HKR aktuell eingestellte Solltemperatur
Temperatur-Wert in 0,5 °C 
Wertebereich: 
16  – 56 = 8 bis 28°C
0   - 16 = 8°C 
17 = 8,5°C ...... 56 >= 28°C
254 = ON
253 = OFF]]></description>
      <argumentList>
        <argument>
          <name>AIN</name>
          <direction>in</direction>
          <relatedStateVariable>AIN</relatedStateVariable>
        </argument>
          <argument>
          <name>sid</name>
          <direction>in</direction>
          <relatedStateVariable>sid</relatedStateVariable>
        </argument>
        <argument>
          <name>Temperatur</name>
          <direction>out</direction>
          <relatedStateVariable>temperatur</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>GetHkrKomfort</name>
      <description><![CDATA[Für HKR-Zeitschaltung eingestellte Komforttemperatur
Temperatur-Wert in 0,5 °C 
Wertebereich: 
16  – 56 = 8 bis 28°C 
0   - 16 = 8°C 
17  = 8,5°C...... 56 >= 28°C
254 = ON
253 = OFF]]></description>
      <argumentList>
        <argument>
          <name>AIN</name>
          <direction>in</direction>
          <relatedStateVariable>AIN</relatedStateVariable>
        </argument>
         <argument>
          <name>sid</name>
          <direction>in</direction>
          <relatedStateVariable>sid</relatedStateVariable>
        </argument>
        <argument>
          <name>Temperatur</name>
          <direction>out</direction>
          <relatedStateVariable>temperatur</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>GetHkrAbsenk</name>
      <description><![CDATA[Für HKR-Zeitschaltung eingestellte Spartemperatur
Temperatur-Wert in 0,5 °C 
Wertebereich: 
16  – 56 = 8 bis 28°C 
0   - 16 = 8°C 
17  = 8,5°C...... 56 >= 28°C
254 = ON
253 = OFF]]></description>
      <argumentList>
        <argument>
          <name>AIN</name>
          <direction>in</direction>
          <relatedStateVariable>AIN</relatedStateVariable>
        </argument>
          <argument>
          <name>sid</name>
          <direction>in</direction>
          <relatedStateVariable>sid</relatedStateVariable>
        </argument>
        <argument>
          <name>Temperatur</name>
          <direction>out</direction>
          <relatedStateVariable>temperatur</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>setHkrtSoll</name>
      <description><![CDATA[HKR Solltemperatur einstellen. Mit dem „param“ Get-Parameter wird die Solltemperatur übergeben.
Temperatur-Wert in 0,5 °C
Wertebereich: 
16  – 56 = 8 bis 28°C 
0   - 16 = 8°C 
17  = 8,5°C...... 56 >= 28°C
254 = ON
253 = OFF]]>
</description>
      <argumentList>
         <argument>
          <name>AIN</name>
          <direction>in</direction>
          <relatedStateVariable>AIN</relatedStateVariable>
        </argument>
        <argument>
          <name>Temperatur</name>
          <direction>in</direction>
          <relatedStateVariable>temperatur</relatedStateVariable>
        </argument>
         <argument>
          <name>sid</name>
          <direction>in</direction>
          <relatedStateVariable>sid</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
  </actionList>
